from tkinter import filedialog
from tkinter import *

def mainscreen ():
    print ("Main Program")

g10 = Tk()
g10.title ("Bouncing Ball")
g10.minsize (500,500)

lblimage = PhotoImage(file="Basketball.png")
Label(g10, text="Welcome to Bouncing Ball", bg="white", fg="black",
      font="none 20 bold"). grid(row=1, column =0, sticky=N)
Label(g10, image=lblimage, bg="white").grid(row = 4, column = 0, sticky=W)
